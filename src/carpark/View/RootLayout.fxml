<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="1200.0" stylesheets="@../Style/Stylesheet.css" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="carpark.CarPark">
    <stylesheets>
        <URL value="@/carpark/Style/Stylesheet.css" />
    </stylesheets>
    <top>
      <HBox id="top_bar" prefHeight="90.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="listBtn" mnemonicParsing="false" onAction="#listButtonAction" prefHeight="50.0" prefWidth="200.0" text="Lista">
               <HBox.margin>
                  <Insets left="20.0" top="20.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="mapBtn" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#mapButtonAction" prefHeight="50.0" prefWidth="200.0" text="Mapa">
               <HBox.margin>
                  <Insets left="20.0" top="20.0" />
               </HBox.margin>
            </Button>
            <HBox prefHeight="100.0" HBox.hgrow="ALWAYS" />
            <Button fx:id="closeBtn" mnemonicParsing="false" onAction="#closeButtonAction" prefHeight="50.0" prefWidth="200.0" text="Zamknij">
               <HBox.margin>
                  <Insets right="20.0" top="20.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </top>
   <center>
   </center>
</BorderPane>
